model FabricStore {
    fabricId    Int
    storeId     Int
    quantity    Int       @default(0)
    createdAt   DateTime  @default(now())
    updatedAt   DateTime  @updatedAt

    fabric      Fabric    @relation(fields: [fabricId], references: [id])
    store       Store     @relation(fields: [storeId], references: [id])

    @@id([fabricId, storeId])
}